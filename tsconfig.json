// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     
  "compilerOptions": {
    "baseUrl": ".",
     "paths": {
       "*": ["src/*"],
       "tests": ["tests/*"],
       "components/*": ["src/components/*"],
       "pages/*": ["src/pages/*"],
       "api": ["src/api/"],
       "api/*": ["src/api/*"],
       "assets/*": ["src/assets/*"],
       "constants/*": ["src/constants/*"],
       "helper/*": ["src/helper/*"],
       "hooks/*": ["src/hooks/*"],
       "hooks/queries/*": ["src/hooks/queries/*"],
       "hooks/mutations/*": ["src/hooks/mutations/*"],
       "states/*": ["src/states/*"],
       "tabs/*": ["src/tabs/*"],
       "utils/*": ["src/utils/*"],
       "types/*": ["src/types/*"],
     },
    "jsx": "react-native",
    "skipLibCheck": true,
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
  }
}
