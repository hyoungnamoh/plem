// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     
  "compilerOptions": {
    "jsx": "react-native",
    "skipLibCheck": true,                                 
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
  }
}
